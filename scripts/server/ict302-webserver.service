[Unit]
Description=ICT302 Node.js Server for a Interactive Dashboard(multi-command)
After=network-online.target
Wants=network-online.target

[Service]
User=root
Group=root
WorkingDirectory=/opt/DynamicPopulationDensity/src/server/frontend/hive-matrix
ExecStart=/bin/bash /opt/DynamicPopulationDensity/scripts/server/start_all_webserver.sh
Restart=on-failure
RestartSec=5
Environment=NODE_ENV=production PORT=8000

[Install]
WantedBy=multi-user.target
